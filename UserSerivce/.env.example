# UserSerivce environment example
PORT=3002

# OIDC / Keycloak
OIDC_ISSUER_URL=http://localhost:8080/realms/fitlol
OIDC_JWKS_URI=http://localhost:8080/realms/fitlol/protocol/openid-connect/certs
# Optional audience validation (only if you configure audience mapping in Keycloak)
# OIDC_AUDIENCE=fitlol-backend
# Role claim path (default)
OIDC_ROLE_PATH=realm_access.roles

# CORS
CORS_ORIGIN=http://localhost:5173

# Rate limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=debug
